.preMadeLayouts
    padding: 100px 15px
    margin: 0 auto;
    display: flex
    flex-direction: column
    align-items: center
    background-color: #f8f8f8
    h1
        margin 0
        font-size: 55px; 
        font-family: 'Roboto'
        text-align: center;
        font-weight: 300
    p
        margin-top: 25px
        font-size: 24px; 
        font-family: 'Roboto'
        text-align: center;
        font-weight: 100
        color: #888888
    .wrapper-pre-made-layouts
        margin: 50px 0
        width: 100%
        display: flex
        align-items: center
        justify-content: space-between
        flex-wrap: wrap
        @media screen and (max-width: 950px)
            justify-content: center
        .pre-made-layout
            cursor: pointer
            img
                max-width: 230px
                height auto
                margin: 30px 35px
                transition: all 0.5s
                &:hover
                    transform: scale(1.05);
                    box-shadow: 10px 10px 74px -15px rgba(0,0,0,0.3);
            div
                text-align: center
                font-family: 'Roboto'
                font-weight: 300
                font-size: 19px 
    .view-more
        background-color: #2dd1ac
        border:1px solid #2dd1ac
        border-radius: 3px
        color: white
        font-size: 18px
        font-family: 'Roboto'
        cursor pointer
        outline: none;
        transition: all 0.5s
        padding: 15px 30px
        transition all 0.5s
        &:hover
            background-color: #24aa8c